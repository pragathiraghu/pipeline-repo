pipeline {
   agent any 

   environment {MY_ENV = 'hello jenkins'}
 
    stages{
        stage ('build stage') {
            steps{

                echo " this my bulid stage "
                echo " this is my enivromental variable : ${MY-ENV} "

                   }
          }

            stage('Run Specific File') {
            steps {
                // Navigate to the file's location and run it
                sh '''
                chmod +x jenkinsfile.sh     # Make the file executable
                ./jenkinsfile.sh            # Run the file
                '''
}
}
}
}
